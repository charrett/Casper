{{!< default}}

{{!-- The tag above means: insert everything in this file
into the {body} tag of the default.hbs template --}}


{{#post}}
{{!-- Everything inside the #post block pulls data from the page --}}

<main id="site-main" class="site-main">
<article class="article {{post_class}}" style="padding-top: 0;">
<section class="gh-head" class "gh-head outer" style="padding-top: 0;">>
        {{!-- A signup call to action is displayed here, unless viewed as a logged-in member --}}
{{#if @site.members_enabled}}
{{#unless @member}}
{{#if access}}
            <h2 class="footer-cta-title">Sign up to keep informed, its free!</h2>
            <a class="footer-cta-button" href="#/portal" data-portal>
                <div class="footer-cta-input">Enter your email</div>
                <span>Quality Coach Book</span>
            </a> </div>
{{/if}}
{{/unless}}
{{/if}}
    <section class="gh-content gh-canvas">
        {{content}}
    </section>

</article>
</main>

{{/post}}
{{!-- Amplitude Logging --}}
{{#if @member}}
<script type="text/javascript">
    amplitude.getInstance().logEvent('Viewed Pricing as Member');
</script>
{{else}}
<script type="text/javascript">
    amplitude.getInstance().logEvent('Viewed Pricing as Guest');
</script>
{{/if}}
